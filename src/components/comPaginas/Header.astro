<style>
  @import url("https://fonts.googleapis.com/css?family=Poppins:200,300,400,500,600,700,800,900&display=swap");

  a::before {
    content: attr(data-text);
    position: absolute;
    color: #fff;
    width: 350px;
    overflow: hidden;
    white-space: nowrap;
    border-right: 4px solid #fff;
    animation: animate 8s linear infinite;
    filter: drop-shadow(0 0 20px #fff) drop-shadow(0 0 50px #fff);
  }
  @keyframes animate {
    0%,
    10%,
    100% {
      width: 0;
    }
    70%,
    90% {
      width: 100%;
    }
  }

  .shadow1 {
    text-shadow: 0 0 3px rgba(247, 245, 133, 0.9);
  }
  .sbox {
    box-shadow: 0 0 10px rgba(247, 245, 133, 0.9);
  }
</style>

<script
  type="text/javascript"
  src="../../../node_modules/tw-elements/js/tw-elements.umd.min.js"
>
// Initialization for ES Users
import { Collapse, Dropdown, Ripple, initTWE } from "tw-elements";

initTWE({ Collapse, Dropdown, Ripple });
</script>